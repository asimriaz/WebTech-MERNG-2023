<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>

<body>
    <h1>@model.title</h1>
    <p>Welcome to @model.title</p>

    @if(model.age == 30){
        <div>Your age is @model.age</div>
    }
    <br>
    <div class="how">
    @for(var i = 0; i < 3; i++){ 
        <div class="item-@i">I be an item @(i+1)</div>
    }
    </div>

    @model.users.forEach(function(user){
        <div>@user.firstName</div>
    })



</body>

</html>